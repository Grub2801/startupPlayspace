<!DOCTYPE html>
<html>
<head>
  <title>PlayspaceApp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <!-- nav -->
  <%= render 'layouts/navbar' %>
  <!-- /nav -->

  <div id="wrapper">
    <!-- sidebar -->
    <%= render 'layouts/sidebar' %>
    <!-- /sidebar-->

    <!-- Page Content -->
    <div id="test" class="page-content-wrapper">
      <div class="container-fluid">
        <div class="row">
          <%= yield %>
        </div>
      </div>
    </div>
  </div>
  <!-- /#page-content-wrapper -->
</body>
</html>
